{
	"name": "aliexpress-scraper",
	"version": "0.1.1",
	"description": "Aliexpress product scraper",
	"type": "module",
	"main": "lib/main.mjs",
	"files": [
		"lib/*"
	],
	"exports": {
		"import": "./lib/main.mjs",
		"require": "./lib/main.cjs"
	},
	"scripts": {
