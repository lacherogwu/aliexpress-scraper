		"scraper"
	],
	"author": "LacheRo",
	"license": "MIT",
	"repository": "lacherogwu/aliexpress-scraper",
	"bugs": {
		"url": "https://github.com/lacherogwu/aliexpress-scraper/issues"
	},
	"dependencies": {
		"axios": "^0.26.1"
	},
	"devDependencies": {
		"rollup": "^2.72.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
